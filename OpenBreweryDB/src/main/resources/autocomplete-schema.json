{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "id",
      "name"
    ],
    "properties": {
      "id": {
        "type": "string",
        "title": "The Id Schema",
        "default": "",
        "pattern": "^(.*)$"
      },
      "name": {
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "pattern": "^(.*)$"
      }
    }
  }
}
